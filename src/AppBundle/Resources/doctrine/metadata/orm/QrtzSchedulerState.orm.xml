<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QrtzSchedulerState" table="QRTZ_SCHEDULER_STATE">
    <field name="checkinInterval" type="integer" column="CHECKIN_INTERVAL" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="instanceName" type="string" column="INSTANCE_NAME" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastCheckinTime" type="integer" column="LAST_CHECKIN_TIME" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="recoverer" type="string" column="RECOVERER" length="80" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
