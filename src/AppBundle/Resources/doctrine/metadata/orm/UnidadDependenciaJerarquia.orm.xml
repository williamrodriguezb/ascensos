      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="descripcionDependencia" type="string" column="DESCRIPCION_DEPENDENCIA" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jerarquiaConsecutivo" type="string" column="JERARQUIA_CONSECUTIVO" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="marcaTimestamp" type="datetime" column="MARCA_TIMESTAMP" nullable="true"/>
    <field name="nivel" type="integer" column="NIVEL" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="unidadDependenciaPk" type="integer" column="UNIDAD_DEPENDENCIA_PK" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
